algoritmo "Subtração"
var
 n1, n2:real
 r:real
inicio
 escreval("Digite dois numeros para fazer SUBTRAÇÃO")
 escreva("Digite o primeiro numero: ")
 leia(n1)
 escreva("Digiteo segundo numero: ")
 leia(n2)
 r<- n1-n2
 escreva("resuldado da Subtração dos numero ",n1, " e", n2, " é igual a" , r)
fimalgoritmo